{
    "version": 2,
    "builds": [
      {
        "src":"./build.sh",
        "use":"@vercel/static-build",
        "config":{
            "distDir":"./client/build"
        }
      },
      {
        "src": "./app.js",  
        "use": "@vercel/node"
      }
    ],
  
    "rewrites": [
      {
        "source": "/(.*)",
        "destination": "./app.js"
      }
    ]
  }
