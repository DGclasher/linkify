{
    "version": 2,
    "builds": [
      {
        "src":"./build.sh",
        "use":"@vercel/static-build"
      },
      {
        "src": "./app.js",  
        "use": "@vercel/node"
      },
      {
        "src": "./client/build",    
        "use": "@vercel/static"
      }
    ],
  
    "rewrites": [
      {
        "source": "/(.*)",
        "destination": "./app.js"
      }
    ]
  }
